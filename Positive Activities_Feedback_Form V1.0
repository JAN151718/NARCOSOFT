<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/MainLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:id="@+id/MainScrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <RelativeLayout
            android:id="@+id/ListLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <EditText
                android:id="@+id/a2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/f_q1"
                android:layout_below="@+id/f_q2"
                android:ems="10"
                android:hapticFeedbackEnabled="false"
                android:inputType="textPersonName"
                android:textColorLink="@android:color/holo_blue_light"
                android:textIsSelectable="true" />

            <TextView
                android:id="@+id/f_q1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_marginStart="45dp"
                android:layout_marginTop="50dp"
                android:background="@color/colorAccent"
                android:padding="7dp"
                android:text="Have you promoted any positive activity in your Mohalla?"
                android:textColor="@android:color/background_light" />

            <TextView
                android:id="@+id/f_q2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/a2"
                android:layout_below="@+id/f_q1"
                android:layout_marginTop="50dp"
                android:background="@color/colorAccent"
                android:padding="7dp"
                android:text="@string/pa_q2"
                android:textColor="@android:color/background_light" />

            <TextView
                android:id="@+id/f_q3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/f_q1"
                android:layout_below="@+id/a2"
                android:layout_marginTop="25dp"
                android:background="@color/colorAccent"
                android:padding="7dp"
                android:text="@string/pa_q3"
                android:textColor="@android:color/background_light" />

            <EditText
                android:id="@+id/f_a3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/f_q3"
                android:layout_below="@+id/f_q3"
                android:ems="10"
                android:inputType="textPersonName"
                android:textColorLink="@android:color/holo_blue_light"
                android:textIsSelectable="true" />

            <TextView
                android:id="@+id/f_q4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/f_q2"
                android:layout_below="@+id/f_a3"
                android:layout_marginTop="25dp"
                android:background="@color/colorAccent"
                android:padding="7dp"
                android:text="@string/pa_q4"
                android:textColor="@android:color/background_light" />

            <EditText
                android:id="@+id/a4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/f_a3"
                android:layout_below="@+id/f_q4"
                android:ems="10"
                android:inputType="time"
                android:textColorLink="@android:color/holo_blue_light"
                android:textIsSelectable="true" />

            <TextView
                android:id="@+id/f_q5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/a4"
                android:layout_below="@+id/a4"
                android:layout_marginTop="25dp"
                android:background="@color/colorAccent"
                android:padding="7dp"
                android:text="@string/pa_q5"
                android:textColor="@android:color/background_light" />

            <EditText
                android:id="@+id/a5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignEnd="@+id/a4"
                android:layout_below="@+id/f_q5"
                android:ems="10"
                android:inputType="text|number"
                android:textColorLink="@android:color/holo_blue_light"
                android:textIsSelectable="true" />

            <TextView
                android:id="@+id/f_q6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/a6"
                android:layout_below="@+id/a5"
                android:layout_marginTop="25dp"
                android:background="@color/colorAccent"
                android:padding="7dp"
                android:text="@string/pa_q6"
                android:textColor="@android:color/background_light" />

            <EditText
                android:id="@+id/a6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/f_q4"
                android:layout_below="@+id/f_q6"
                android:ems="10"
                android:inputType="number"
                android:textColorLink="@android:color/holo_blue_light"
                android:textIsSelectable="true" />

            <RadioButton
                android:id="@+id/A1_No"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/f_q1"
                android:layout_marginStart="21dp"
                android:layout_toEndOf="@+id/A1_Yes"
                android:hapticFeedbackEnabled="true"
                android:text="Only Attempted" />

            <RadioButton
                android:id="@+id/A1_Yes"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/f_q1"
                android:layout_below="@+id/f_q1"
                android:hapticFeedbackEnabled="true"
                android:text="@string/yesButton" />

            <TextView
                android:id="@+id/f_q7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/a7"
                android:layout_below="@+id/a6"
                android:layout_marginTop="25dp"
                android:background="@color/colorAccent"
                android:padding="7dp"
                android:text="@string/pa_q7"
                android:textColor="@android:color/background_light" />

            <EditText
                android:id="@+id/a7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/f_q4"
                android:layout_below="@+id/f_q7"
                android:ems="10"
                android:inputType="number"
                android:textColorLink="@android:color/holo_blue_light"
                android:textIsSelectable="true" />

            <TextView
                android:id="@+id/f_q8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/a7"
                android:layout_below="@+id/a7"
                android:layout_marginTop="25dp"
                android:background="@color/colorAccent"
                android:padding="7dp"
                android:text="@string/pa_q8"
                android:textColor="@android:color/background_light" />

            <EditText
                android:id="@+id/a8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/f_q4"
                android:layout_below="@+id/f_q8"
                android:ems="10"
                android:inputType="textPersonName"
                android:textColorLink="@android:color/holo_blue_light"
                android:textIsSelectable="true" />

            <TextView
                android:id="@+id/f_q9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/a7"
                android:layout_below="@+id/a8"
                android:layout_marginTop="25dp"
                android:background="@color/colorAccent"
                android:padding="7dp"
                android:text="@string/pa_q9"
                android:textColor="@android:color/background_light" />

            <RadioButton
                android:id="@+id/yesButton2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/f_q9"
                android:layout_below="@+id/f_q9"
                android:text="@string/yb2" />

            <RadioButton
                android:id="@+id/noButton2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/A1_No"
                android:layout_below="@+id/f_q9"
                android:text="@string/nb2" />

            <TextView
                android:id="@+id/f_q10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/a7"
                android:layout_below="@+id/yesButton2"
                android:layout_marginTop="25dp"
                android:background="@color/colorAccent"
                android:padding="7dp"
                android:text="@string/pa_q10"
                android:textColor="@android:color/background_light" />

            <EditText
                android:id="@+id/a10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/f_q4"
                android:layout_below="@+id/f_q10"
                android:ems="10"
                android:inputType="textPersonName"
                android:textColorLink="@android:color/holo_blue_light"
                android:textIsSelectable="true" />

            <TextView
                android:id="@+id/f_q11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/a7"
                android:layout_below="@+id/a10"
                android:layout_marginTop="25dp"
                android:background="@color/colorAccent"
                android:padding="7dp"
                android:text="@string/pa_q11"
                android:textColor="@android:color/background_light" />

            <RadioButton
                android:id="@+id/yesButton3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/f_q11"
                android:layout_below="@+id/f_q11"
                android:text="@string/yb3" />

            <RadioButton
                android:id="@+id/noButton3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/A1_No"
                android:layout_below="@+id/f_q11"
                android:text="@string/nb3" />

            <TextView
                android:id="@+id/f_q12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/a7"
                android:layout_below="@+id/yesButton3"
                android:layout_marginTop="25dp"
                android:background="@color/colorAccent"
                android:padding="7dp"
                android:text="@string/pa_q12"
                android:textColor="@android:color/background_light" />

            <EditText
                android:id="@+id/a12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/f_q4"
                android:layout_below="@+id/f_q12"
                android:ems="10"
                android:inputType="textPersonName"
                android:textColorLink="@android:color/holo_blue_light"
                android:textIsSelectable="true" />

            <Button
                android:id="@+id/ProceedButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/a12"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="25dp"
                android:layout_marginLeft="50dp"
                android:layout_marginRight="50dp"
                android:layout_marginTop="50dp"
                android:background="@color/colorAccent"
                android:padding="10dp"
                android:paddingEnd="50dp"
                android:paddingStart="50dp"
                android:text="@string/PButton"
                android:textColor="@android:color/background_light"
                android:onClick="Proceed"/>

           

           
        </RelativeLayout>
    </ScrollView>

</LinearLayout>
